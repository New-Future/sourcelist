pre="ipv6."
CODENAME=$(lsb_release -c|awk '{print tolower($2)}')
DISTRIBUTOR=$(lsb_release -i| awk '{print tolower($3)}')

deb https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME} main restricted universe multiverse
deb-src https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME} main restricted universe multiverse

deb https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME}-security main restricted universe multiverse
deb-src https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME}-security main restricted universe multiverse

deb https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME}-updates main restricted universe multiverse
deb-src https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME}-updates main restricted universe multiverse

deb https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME}-backports main restricted universe multiverse
deb-src https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME}-backports main restricted universe multiverse

## Not recommended
# deb https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME}-proposed main restricted universe multiverse
# deb-src https://${pre}mirrors.ustc.edu.cn/${DISTRIBUTOR}/ ${CODENAME}-proposed main restricted universe multiverse
